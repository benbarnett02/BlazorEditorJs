<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<Version>1.0.1.3</Version>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<AddRazorSupportForMvc>true</AddRazorSupportForMvc>
	</PropertyGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.4" />
		<PackageReference Include="Microsoft.Web.LibraryManager.Build" Version="2.1.161" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="wwwroot\lib\codex.editor.header\" />
	  <Folder Include="wwwroot\lib\codex.editor.personality\src\" />
	  <Folder Include="wwwroot\lib\editorjs\checklist\.github\" />
	  <Folder Include="wwwroot\lib\editorjs\types\events\" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="wwwroot\lib\editorjs\code\.github\workflows\npm-publish.yml" />
	  <None Include="wwwroot\lib\editorjs\example\tools\image\dev\.tmp\.gitignore" />
	  <None Include="wwwroot\lib\editorjs\link\.github\workflows\npm-publish.yml" />
	</ItemGroup>

</Project>
